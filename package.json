{
  "name": "gamekey",
  "version": "2.0.0",
  "description": "GameKeyapp",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-all": "electron-packager . --overwrite --asar --all --icon=icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"GameKey\"",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --icon=icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"GameKey\"",
    "package-linux": "electron-packager . --overwrite --asar --platform=linux --arch=x64 --icon=icon.ico --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheKiLLerDz/GameKey.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheKiLLerDz/GameKey/issues"
  },
  "homepage": "https://github.com/TheKiLLerDz/GameKey#readme",
  "dependencies": {
    "aes256": "^1.0.4",
    "json2xls": "^0.1.2",
    "open": "^7.0.2",
    "read-excel-file": "^4.0.5"
  },
  "devDependencies": {
    "electron": "^7.1.11",
    "electron-packager": "^14.2.0"
  }
}
